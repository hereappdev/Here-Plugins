{
   "name": "BTC Price",
   "version": "1.0",
   "description": "Show BTC Price",
   "icon": "icon.png",
   "identifier": "app.here.btcprice",
   "createdBy": "Here App",
   "website": "https://here.app",
   "appcast": "https://raw.githubusercontent.com/hereappdev/plugins/master/source/app.here.btcprice/appcast.xml",
   "script": "index.js",
   "defaultCategory": "fav",
   "displayOn": [
      "mini",
      "menuBar",
      "dock"
   ],
   "preferences": [
       {
            "title": "Coin Name",
            "type": "text",
            "key": "coinName",
            "default": "BTC Price"
        },
        {
            "title": "Coin Code",
            "type": "text",
            "key": "coinCode",
            "default": "btc"
        }
   ]
}